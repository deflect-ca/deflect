; Generated by Deflect Next

$TTL    300
@       IN      SOA     {{ soa_nameserver }}. {{ soa_mailbox }}. (
                     {{ serial }}         ; Serial
                            300         ; Refresh
                            300         ; Retry
                        2419200         ; Expire
                            300 )       ; Negative Cache TTL

; NS{% for ns in default_ns %}
@       IN      NS      {{ ns }}.{% endfor %}

; Conceal the IP of the controller
@       IN      A       127.0.0.1

; Other essential records
acme    IN      A       {{ ip }}

; Test origin{% for edge in edges %}
test-origin     IN      A       {{ edge['ip'] }}{% endfor %}
_acme-challenge.test-origin     IN      NS       acme.{{ root_domain }}.

